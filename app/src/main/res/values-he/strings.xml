    <string name="metric_balance_tip4">חוסר איזון קל הוא נורמלי ולעתים מתקן את עצמו</string>
    <string name="metric_balance_tip5">אל תתייחס לתנודות קטנות במהלך הרכיבה</string>
    <string name="metric_balance_research">מחקרים מראים שלרוב הרוכבים יש חוסר איזון טבעי של 1-5%%. רוכבים מקצועיים מגיעים בממוצע ל-48-52%%.</string>

    <!-- MetricInfo - Torque Effectiveness (detailed) -->
    <string name="metric_te_name">יעילות מומנט</string>
    <string name="metric_te_short">יעילות סיבוב דוושה</string>
    <string name="metric_te_explanation">יעילות מומנט (יע״מ) מודדת כמה מתנועת הדיווש תורמת להנעה קדימה. ציון של 100%% היה אומר שכל חלק בסיבוב מוסיף הספק, אבל זה לא ריאלי ולא אופטימלי.</string>
    <string name="metric_te_optimal">70-80%% (אופטימלי, לא גבוה יותר)</string>
    <string name="metric_te_tip1">התמקד במשיכה דרך תחתית הסיבוב</string>
    <string name="metric_te_tip2">דמיין שאתה מגרד בוץ מהנעל בתחתית</string>
    <string name="metric_te_tip3">יע״מ גבוה (מעל 80%%) לעתים מפחית את תפוקת ההספק הכללית</string>
    <string name="metric_te_tip4">דיווש חלק ומעגלי יעיל יותר מאילוץ יע״מ גבוה</string>
    <string name="metric_te_tip5">יע״מ משתנה באופן טבעי עם קדנס והספק</string>
    <string name="metric_te_research">מחקר של Wattbike מראה שיע״מ מעל 80%% למעשה מפחית תפוקת הספק. רוכבים מקצועיים מכוונים ל-70-80%%.</string>

